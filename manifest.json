{
    "manifest_version": 3,
    "name": "gpt2redmine",
    "version": "1.0.0",
    "author": "Norio4 Yahata",
    "description": "Save GPT answer to Redmine ticket",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Save ticket!"
        }
    }
}
